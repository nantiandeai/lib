    <sql>select t.copylibraryid &#x7c;&#x7c; &#x27;&#x27; as id,&#xd;&#xa;       t.unitid as org_id,&#xd;&#xa;       t.libraryid &#x7c;&#x7c; &#x27;&#x27; as BOOK_DIRECTORY_ID,&#xd;&#xa;       t.SITEID as COLLECTION_SITE_ID,&#xd;&#xa;       t.BARCODE as BAR_CODE,&#xd;&#xa;       decode&#x28;t.ISBESPOKEN,&#xd;&#xa;              &#x27;2&#x27;,&#xd;&#xa;              &#x27;5&#x27;,&#xd;&#xa;              decode&#x28;t.state,&#xd;&#xa;                     &#x27;0001&#x27;,&#xd;&#xa;                     &#x27;0&#x27;,&#xd;&#xa;                     &#x27;0002&#x27;,&#xd;&#xa;                     &#x27;1&#x27;,&#xd;&#xa;                     &#x27;0003&#x27;,&#xd;&#xa;                     &#x27;2&#x27;,&#xd;&#xa;                     &#x27;0004&#x27;,&#xd;&#xa;                     decode&#x28;cinfo.barcode, null, &#x27;0&#x27;, &#x27;1&#x27;&#x29;,&#xd;&#xa;                     &#x27;0005&#x27;,&#xd;&#xa;                     &#x27;4&#x27;&#x29;&#x29; as STATUS,&#xd;&#xa;       decode&#x28;cinfo.ifrenew, &#x27;1&#x27;, &#x27;1&#x27;, &#x27;0&#x27;&#x29; as IS_RENEW,&#xd;&#xa;       decode&#x28;t.state, &#x27;0004&#x27;, &#x27;1&#x27;, &#x27;0&#x27;&#x29; as IS_STAINED,&#xd;&#xa;       null as CREATE_BY,&#xd;&#xa;       to_date&#x28;INLIBRARYDATE, &#x27;yyyy-MM-dd hh24&#x3a;mi&#x3a;ss&#x27;&#x29; as CREATE_DATE,&#xd;&#xa;       nvl&#x28;decode&#x28;t.state,&#x27;0005&#x27;,nvl&#x28;p.operatedate,sysdate-1&#x29;,null&#x29;, to_date&#x28;INLIBRARYDATE, &#x27;yyyy-MM-dd hh24&#x3a;mi&#x3a;ss&#x27;&#x29;&#x29; as UPDATE_DATE,&#xd;&#xa;       &#x27;0&#x27; as DEL_FLAG,&#xd;&#xa;       decode&#x28;ISBESPOKEN, &#x27;1&#x27;, &#x27;1&#x27;, &#x27;0&#x27;&#x29; as IS_ORDER,&#xd;&#xa;       null as BATCH_ID,&#xd;&#xa;       null as BATCH_NO&#xd;&#xa;       &#xd;&#xa;  from schl_copylibrary t&#xd;&#xa; inner join schl_library t1 on t.libraryid &#x3d; t1.libraryid&#xd;&#xa;  left join &#x28;select c.barcode, c.unitid, c.ifrenew&#xd;&#xa;               from schl_circulate c&#xd;&#xa;              where c.returndate is null&#xd;&#xa;                   --and c.ifrenew &#x3d; &#x27;1&#x27;&#xd;&#xa;                and c.state &#x3d; &#x27;0&#x27;&#x29; cinfo on &#x28;t.barcode &#x3d; cinfo.barcode and&#xd;&#xa;                                            t.unitid &#x3d; cinfo.unitid&#x29;&#xd;&#xa;    left join SCHL_CIRDISPOSE p on t.unitid&#x3d;p.unitid and t.barcode &#x3d; p.barcode and p.operatetype&#x3d;&#x27;0&#x27; </sql>
