    <connection>&#x884c;&#x9986;&#x6570;&#x636e;&#x5e93;&#x8fde;&#x63a5;</connection>
    <sql>with temp as &#x28;&#xd;&#xa;select t.libraryid id,replace&#x28;t.isbn,&#x27;-&#x27;&#x29; isbn,&#xd;&#xa;       t.subject title,&#xd;&#xa;       t.author,&#xd;&#xa;       t.publishname pn,&#xd;&#xa;       t.publishdate pd,&#xd;&#xa;       to_number&#x28;t.unitprice&#x29; price&#xd;&#xa;  from schl_library_null t where t.isbn is null or t.subject is null or t.publishname is null or&#xd;&#xa;       t.publishdate is null or t.author is null&#xd;&#xa;  &#x29;,&#xd;&#xa; td as &#x28;&#xd;&#xa; select replace&#x28;d.isbn,&#x27;-&#x27;,&#x27;&#x27;&#x29; isbn,&#xd;&#xa;        d.title,&#xd;&#xa;        d.author,&#xd;&#xa;        d.publishing_name pn,&#xd;&#xa;        d.publishing_time pd&#xd;&#xa;   from info_book_directory d&#xd;&#xa;   &#xd;&#xa; &#x29; ,&#xd;&#xa;  matched as &#x28;select &#xd;&#xa;        nvl&#x28;t.isbn,d.isbn&#x29; isbn,&#xd;&#xa;        nvl&#x28;t.title,d.title&#x29; title,&#xd;&#xa;        nvl&#x28;t.author,d.author&#x29; author,&#xd;&#xa;        nvl&#x28;t.pn,d.pn&#x29; pn,&#xd;&#xa;        nvl&#x28;t.pd,d.pd&#x29; pd,&#xd;&#xa;        nvl&#x28;t.price,0&#x29; price,&#xd;&#xa;        t.id tid&#xd;&#xa;        from td d, temp t&#xd;&#xa; where 1&#x3d;1&#xd;&#xa;   and  &#x28;d.isbn &#x3d; t.isbn or t.isbn is null&#x29; &#xd;&#xa;   and &#x28;d.title &#x3d; t.title&#x29;&#xd;&#xa;   and &#x28;d.author &#x3d; t.pn or t.author is null&#x29;&#xd;&#xa;   and &#x28;d.pn &#x3d; t.pn or t.pn is null&#x29;&#xd;&#xa;   and &#x28;d.pd &#x3d; t.pn or t.pd is null&#x29;&#xd;&#xa;   &#x29;&#xd;&#xa;   &#xd;&#xa;select max&#x28;m.isbn&#x7c;&#x7c;&#x27;&#x40;&#x7c;&#x7c;&#x40;&#x27;&#x7c;&#x7c;m.title&#x7c;&#x7c;&#x27;&#x40;&#x7c;&#x7c;&#x40;&#x27;&#x7c;&#x7c;m.author&#x7c;&#x7c;&#x27;&#x40;&#x7c;&#x7c;&#x40;&#x27;&#x7c;&#x7c;m.pn&#x7c;&#x7c;&#x27;&#x40;&#x7c;&#x7c;&#x40;&#x27;&#x7c;&#x7c;m.pd&#x7c;&#x7c;&#x27;&#x40;&#x7c;&#x7c;&#x40;&#x27;&#x7c;&#x7c;m.price&#x29; b,m.tid from matched m where m.isbn is not null and m.title is not null and  m.author is not null and m.pn is not null and m.pd is not null &#xd;&#xa;group by tid</sql>
    <limit>0</limit>
