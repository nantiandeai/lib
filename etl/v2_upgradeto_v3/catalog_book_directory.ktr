    <connection>&#x56fe;&#x4e66;V2.0&#x6570;&#x636e;&#x8fde;&#x63a5;</connection>
    <sql>select t.libraryid&#x7c;&#x7c;&#x27;&#x27; as id,&#xd;&#xa;       t.unitid as ORG_ID,&#xd;&#xa;       NVL&#x28;replace&#x28;t.isbn, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#x27;&#x4e0d;&#x8be6;&#x27;&#x29; as ISBN,&#xd;&#xa;       nvl&#x28;t.SUBJECT,&#x27;&#x4e0d;&#x8be6;&#x27;&#x29; as TITLE,&#xd;&#xa;       t.SUBTITLE as SUB_TITLE,&#xd;&#xa;       t.APPOSETITLE as TIED_TITLE,&#xd;&#xa;       t.SECTIONNAME as PART_NAME,&#xd;&#xa;       t.SECTIONNUMBER as PART_NUM,&#xd;&#xa;       t.SERIESNAME as SERIES_NAME,&#xd;&#xa;       nvl&#x28;t.author,&#x27;&#x4e0d;&#x8be6;&#x27;&#x29; as AUTHOR,&#xd;&#xa;       t.RESTDUTY as SUB_AUTHOR,&#xd;&#xa;       t.SERIESEDIT as SERIES_EDITOR,&#xd;&#xa;       t.TRANSLATOR as TRANSLATOR,&#xd;&#xa;       nvl&#x28;t.PUBLISHNAME,&#x27;&#x4e0d;&#x8be6;&#x27;&#x29; as PUBLISHING_NAME,&#xd;&#xa;       t.PUBLISHADDR as PUBLISHING_ADDRESS,&#xd;&#xa;       nvl&#x28;t.PUBLISHDATE,&#x27;&#x4e0d;&#x8be6;&#x27;&#x29; as PUBLISHING_TIME,&#xd;&#xa;       t.ASSORTNUM as LIBRARSORT_CODE,&#xd;&#xa;       nvl&#x28;t.unitprice,&#x27;0&#x27;&#x29; unitprice,&#xd;&#xa;       t.PRINTNUM as EDITION,&#xd;&#xa;       decode&#x28;t.LANGUAGE, &#x27;chi&#x27;, &#x27;0&#x27;, &#x27;eng&#x27;, &#x27;1&#x27;&#x29; as LANGUAGE,&#xd;&#xa;       t.LIBSIZE as MEASURE,&#xd;&#xa;       t.PAGESUM as PAGE_NO,&#xd;&#xa;       t.BINDFORM as BINDING_FORM,&#xd;&#xa;       decode&#x28;t.readingage,&#xd;&#xa;              &#x27;7-10&#x5c81;&#x27;,&#xd;&#xa;              &#x27;0&#x27;,&#xd;&#xa;              &#x27;6-12&#x5c81;&#x27;,&#xd;&#xa;              &#x27;0&#x27;,&#xd;&#xa;              &#x27;13-18&#x5c81;&#x27;,&#xd;&#xa;              &#x27;1&#x27;,&#xd;&#xa;              &#x27;18&#x5c81;&#x4ee5;&#x4e0a;&#x27;,&#xd;&#xa;              &#x27;2&#x27;&#x29; as BEST_AGE,&#xd;&#xa;       t.ATTACHMENTNOTE as ATTACHMENT_NOTE,&#xd;&#xa;       t.SUBJECTCI as SUBJECT,&#xd;&#xa;       t.INTRO as CONTENT,&#xd;&#xa;       t.OPERATEID as CREATE_BY,&#xd;&#xa;       to_date&#x28;t.OPERATEDATE, &#x27;yyyy-MM-dd hh24&#x3a;mi&#x3a;ss&#x27;&#x29; as CREATE_DATE,&#xd;&#xa;       t.OPERATEID as UPDATE_BY,&#xd;&#xa;       to_date&#x28;t.OPERATEDATE, &#x27;yyyy-MM-dd hh24&#x3a;mi&#x3a;ss&#x27;&#x29; as UPDATE_DATE,&#xd;&#xa;       &#x27;&#x27; as REMARKS,&#xd;&#xa;       &#x27;0&#x27; as DEL_FLAG,&#xd;&#xa;       TYPENUMBER,&#xd;&#xa;       decode&#x28;t.CIRCULATETYPE,&#xd;&#xa;              &#x27;2001&#x27;,&#xd;&#xa;              &#x27;0&#x27;,&#xd;&#xa;              &#x27;2002&#x27;,&#xd;&#xa;              &#x27;0&#x27;,&#xd;&#xa;              &#x27;2003&#x27;,&#xd;&#xa;              &#x27;0&#x27;,&#xd;&#xa;              &#x27;2005&#x27;,&#xd;&#xa;              &#x27;0&#x27;,&#xd;&#xa;              &#x27;2006&#x27;,&#xd;&#xa;              &#x27;0&#x27;,&#xd;&#xa;              &#x27;2007&#x27;,&#xd;&#xa;              &#x27;0&#x27;,&#xd;&#xa;              &#x27;2008&#x27;,&#xd;&#xa;              &#x27;0&#x27;,&#xd;&#xa;              &#x27;2009&#x27;,&#xd;&#xa;              &#x27;0&#x27;,&#xd;&#xa;              &#x27;2010&#x27;,&#xd;&#xa;              &#x27;0&#x27;,&#xd;&#xa;              &#x27;2011&#x27;,&#xd;&#xa;              &#x27;0&#x27;,&#xd;&#xa;              &#x27;2012&#x27;,&#xd;&#xa;              &#x27;0&#x27;,&#xd;&#xa;              &#x27;2004&#x27;,&#xd;&#xa;              &#x27;2&#x27;&#x29; as PURPOSE,&#xd;&#xa;       t.MARC64 as MARC64&#xd;&#xa;  from schl_library t&#xd;&#xa; left join CODE_USER US ON T.OPERATEID &#x3d; US.USERID&#xd;&#xa;   </sql>
